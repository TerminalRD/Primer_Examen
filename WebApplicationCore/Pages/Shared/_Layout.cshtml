<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <base href="/" />
    <title>@ViewData["Title"] - WebApplicationCore</title>
    @await Html.PartialAsync("_HeaderPartial")
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <div class="container-fluid">
            <a class="navbar-brand" href="#">EncuentraEmpleo</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" asp-page= "">Inicio</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-page="/Jobs/Grid">Puestos</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">Titulos</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">Departamentos</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link disabled" href="#" tabindex="-1" aria-disabled="true">Disabled</a>
                    </li>
                </ul>
                <div class="d-flex">
                <div class="dropdown">
                    <a class="nav-link dropdown-toggle text-white" href="#" id="navbarLoginUser" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                        User
                    </a>
                    <ul class="dropdown-menu" aria-labelledby="navbarLoginUser">
                        <li><a class="dropdown-item" href="#">Cerrar Sesión</a></li>
                    </ul>
                </div>
                </div>
            </div>
        </div>
    </nav>





    @RenderBody()

    @await Html.PartialAsync("_ScriptsPartial")

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
